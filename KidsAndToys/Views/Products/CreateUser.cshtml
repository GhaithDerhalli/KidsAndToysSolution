@model CreateUserVM


<h3>Login</h3>
<form asp-controller="Products" asp-action="Login">
    <label asp-for="Username">Användarnamn</label>
    <input asp-for="Username" />
    <span asp-validation-for="Username"></span>
    
    <label asp-for="Epost">E-post</label>
    <input asp-for="Epost" />
    <span asp-validation-for="Epost"></span>

    <label asp-for="PhoneNumber">Telefonnummer</label>
    <input asp-for="PhoneNumber" />
    <span asp-validation-for="PhoneNumber"></span>

    <label asp-for="ZipCode">Postnummer</label>
    <input asp-for="ZipCode" />
    <span asp-validation-for="ZipCode"></span>

    <label asp-for="Address">Postadress</label>
    <input asp-for="Address" />
    <span asp-validation-for="Address"></span>

    <label asp-for="City">Stad</label>
    <input asp-for="City" />
    <span asp-validation-for="City"></span>

    <label asp-for="Password">Lösenord</label>
    <input asp-for="Password" />
    <span asp-validation-for="Password"></span>

    <label asp-for="PasswordRepeat">Repetera Lösenord</label>
    <input asp-for="PasswordRepeat" />
    <span asp-validation-for="PasswordRepeat"></span>

<form enctype="multipart/form-data">
    <label asp-for="ProfilePic">Profil bild</label>
    <input asp-for="ProfilePic" />
    <span asp-validation-for="ProfilePic"></span>

    <input type="submit" value="Bifoga bild"/>
</form>
    <br />
    <input type="submit" value="Skapa" />
</form>
<div asp-validation-summary="All"></div>
 